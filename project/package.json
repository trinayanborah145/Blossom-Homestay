{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "crypto": false,
    "stream": false,
    "util": false
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "dist/index.html",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "postinstall": "npm run build",
    "netlify-build": "npm install --legacy-peer-deps && npm run build"
  },
  "dependencies": {
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "lucide-react": "^0.344.0",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3",
    "stream-http": "^3.2.0",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "crypto-browserify": "^3.12.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.35",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "unenv": "^1.10.0",
    "util": "^0.12.5",
    "vite": "^5.4.2",
    "vite-plugin-node-polyfills": "^0.17.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  }
}
